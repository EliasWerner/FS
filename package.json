{
  "name": "furniture-shop",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "compression": "1.7.4",
    "ejs": "3.1.6",
    "express": "4.17.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "typescript": "4.3.5",
    "web-vitals": "1.1.2"
  },
  "scripts": {
    "start:server": "node ./dist/server.js",
    "watch:server": "nodemon",
    "dev": "cross-env NODE_ENV=development npm run watch:server",
    "build": "npm run build:server && npm run build:client",
    "build:client": "webpack --config ./webpack.config.client.prod.ts",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "dev:test": "cross-env NODE_ENV=development && webpack --config ./webpack.test.config.js",
    "build:test": "cross-env NODE_ENV=production && webpack --config ./webpack.test.config.js",
    "watch:test": "cross-env NODE_ENV=development && webpack --watch",
    "start:test": "webpack serve --config ./webpack.test.config.js --progress"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.14.8",
    "@babel/preset-env": "7.14.8",
    "@babel/preset-react": "7.14.5",
    "@babel/register": "7.14.5",
    "@types/express": "4.17.13",
    "@types/node": "12.20.17",
    "@types/react": "17.0.15",
    "@types/react-dom": "17.0.9",
    "babel-loader": "8.2.2",
    "clean-webpack-plugin": "4.0.0-alpha.0",
    "copy-webpack-plugin": "9.0.1",
    "cross-env": "7.0.3",
    "css-loader": "6.2.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.3.2",
    "mini-css-extract-plugin": "2.1.0",
    "nodemon": "2.0.12",
    "postcss-loader": "6.1.1",
    "react-hot-loader": "^4.13.0",
    "style-loader": "3.2.1",
    "ts-loader": "^9.2.5",
    "url-loader": "4.1.1",
    "webpack": "5.48.0",
    "webpack-cli": "4.7.2",
    "webpack-dev-middleware": "5.0.0",
    "webpack-dev-server": "3.11.2",
    "webpack-hot-middleware": "2.25.0",
    "webpack-manifest-plugin": "3.2.0",
    "webpack-node-externals": "3.0.0",
    "xml-loader": "1.2.1"
  }
}
